Bootstrap: docker
From: mongo-express:latest

%environment
    export ME_CONFIG_MONGODB_SERVER=localhost
    export ME_CONFIG_MONGODB_PORT=6534
    export ME_CONFIG_MONGODB_ADMINUSERNAME=fedn_admin
    export ME_CONFIG_MONGODB_ADMINPASSWORD=password
    export ME_CONFIG_BASICAUTH_USERNAME=fedn_admin
    export ME_CONFIG_BASICAUTH_PASSWORD=password

%startscript
    cd /node_modules/mongo-express
    /sbin/tini -- /docker-entrypoint.sh mongo-express
