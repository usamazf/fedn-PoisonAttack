Bootstrap: docker
From: minio/minio:14128-5ee91dc

%arguments
    DATA_PATH=/data
    CONS_ADDR=localhost:9001

%environment
    export GET_HOSTS_FROM=dns
    export MINIO_HOST=minio
    export MINIO_PORT=9000
    export MINIO_ROOT_USER=fedn_admin
    export MINIO_ROOT_PASSWORD=password

%startscript
    minio server {{DATA_PATH}} --console-address {{CONS_ADDR}}
